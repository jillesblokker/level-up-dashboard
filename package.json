{
  "name": "level-up-dashboard",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean-start": "rm -rf .next node_modules/.cache && next dev -p 3005",
    "dev:custom": "node server.js",
    "build": "next build",
    "start": "next start",
    "postinstall": "node scripts/generate-musical-audio.js",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:update": "jest --updateSnapshot",
    "test:verbose": "jest --verbose",
    "test:performance": "jest --testNamePattern='performance|Performance'",
    "test:unit": "jest --testPathPattern='__tests__/lib|__tests__/hooks'",
    "test:components": "jest --testPathPattern='__tests__/components'",
    "test:integration": "jest --testPathPattern='__tests__/integration'",
    "test:e2e": "jest --testPathPattern='__tests__/e2e'",
    "test:staged": "jest --onlyChanged --passWithNoTests",
    "test:changed": "jest --onlyChanged",
    "test:failed": "jest --onlyFailures",
    "test:report": "jest --coverage --coverageReporters=html && open coverage/lcov-report/index.html",
    "test:clean": "rm -rf coverage .jest-cache",
    "test:setup": "jest --setupFilesAfterEnv=jest.setup.js",
    "test:validate": "jest --validate",
    "test:help": "jest --help",
    "db:migrate": "./db-migrate.sh"
  },
